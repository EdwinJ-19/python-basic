   "source": [
    "#single inheritance: when a child class inherits from only one parent class, it is called single inheritance.\n",
    "\n",
    "class Parent:\n",
    "    def show(self):\n",
    "        print(\"Parent class side\")\n",
    "class Child(Parent):\n",
    "    def show(self):\n",
    "        print(\"Child class side\")"
   ]
  }
